<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>


<BorderPane prefHeight="583.0" prefWidth="794.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ininprotec.BolsaEmpleoController">
   <center>
      <BorderPane>
         <center>
            <VBox alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
               <children>
                  <FlowPane alignment="CENTER">
                     <children>
                        <TextField fx:id="textBuscador" onKeyReleased="#filtrar" prefHeight="26.0" prefWidth="323.0" promptText="Buscar..." />
                     </children>
                  </FlowPane>
                  <TableView fx:id="tabla" prefHeight="568.0" prefWidth="770.0">
                    <columns>
                      <TableColumn fx:id="cPosicion" prefWidth="75.0" text="Posición" />
                      <TableColumn fx:id="cNombre" prefWidth="75.0" text="Nombre Y Apellidos" />
                        <TableColumn fx:id="cDNI" prefWidth="75.0" text="DNI" />
                        <TableColumn fx:id="cCorreo" prefWidth="75.0" text="E-MAIL" />
                        <TableColumn fx:id="cSexo" prefWidth="75.0" text="Sexo" />
                        <TableColumn fx:id="cFormacion" prefWidth="75.0" text="Formación" />
                        <TableColumn fx:id="cIdioma" prefWidth="75.0" text="Idioma" />
                        <TableColumn fx:id="cNota" prefWidth="75.0" text="Nota " />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </VBox>
         </center>
         <top>
            <FlowPane alignment="CENTER" prefHeight="70.0" prefWidth="770.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="BOLSA DE EMPLEO" />
               </children>
            </FlowPane>
         </top>
      </BorderPane>
   </center>
   <top>
      <AnchorPane prefHeight="89.0" prefWidth="770.0" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
