                              <Font name="Lucida Sans Typewriter Bold Oblique" size="13.0" />
                           </tickLabelFont></CategoryAxis>
                    </xAxis>
                    <yAxis>
                      <NumberAxis fx:id="yNumeros" label="Beneficios" side="TOP" tickLabelGap="5.0" upperBound="100000.0">
